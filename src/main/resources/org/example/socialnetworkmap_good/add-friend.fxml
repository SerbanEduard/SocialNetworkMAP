<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddFriendController">
   <children>
      <TableView fx:id="tableUsers" layoutX="24.0" layoutY="14.0" prefHeight="272.0" prefWidth="369.0">
        <columns>
          <TableColumn fx:id="columnFirstName" prefWidth="75.0" text="First name" />
          <TableColumn fx:id="columnLastName" prefWidth="75.0" text="Last name" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="buttonAdd" layoutX="181.0" layoutY="331.0" mnemonicParsing="false" onAction="#handleButtonAddClick" text="Add friend">
         <font>
            <Font name="Droid Sans Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="labelError" layoutX="24.0" layoutY="367.0" prefHeight="18.0" prefWidth="370.0" textAlignment="CENTER" textFill="RED" />
      <Label layoutX="24.0" layoutY="302.0" text="Search box:">
         <font>
            <Font name="Droid Sans Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldLastName" layoutX="107.0" layoutY="299.0" />
   </children>
</AnchorPane>
